{
    "name": "schema-first-poc",
    "version": "1.0.0",
    "scripts": {
        "dev": "nodemon",
        "codegen": "graphql-codegen",
        "database:setup": "docker-compose run --rm migration",
        "generate": "ts-node ./generator",
        "orm:generate": "prisma generate",
        "orm:sync": "prisma db pull && prettier --write './src/database/schema.prisma'"
    },
    "dependencies": {
        "@apollo/server": "^4.0.2",
        "@prisma/client": "^4.5.0",
        "dayjs": "^1.11.6",
        "dotenv": "^16.0.3",
        "envalid": "^7.3.1",
        "graphql": "^16.6.0",
        "pino": "^8.7.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.13.7",
        "@graphql-codegen/graphql-modules-preset": "^2.5.4",
        "@graphql-codegen/typescript": "^2.7.5",
        "@graphql-codegen/typescript-resolvers": "^2.7.5",
        "@rimac-technology/eslint-config": "^30.0.0",
        "@types/node": "^18.11.3",
        "eslint": "^8.26.0",
        "generate-template-files": "^3.2.1",
        "nodemon": "^2.0.20",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.7.1",
        "prettier-plugin-prisma": "^4.4.0",
        "prisma": "^4.5.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "prisma": {
        "schema": "./src/database/schema.prisma"
    },
    "packageManager": "yarn@3.2.4"
}
